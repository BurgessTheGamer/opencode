{
  "name": "openbrowser",
  "version": "1.0.0",
  "description": "Free, open-source alternative to HyperBrowser MCP with AI enhancement",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target node",
    "start": "node dist/server.js",
    "test": "bun test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@mozilla/readability": "^0.5.0",
    "@types/jsdom": "21.1.7",
    "ai": "^4.0.0",
    "cheerio": "^1.0.0",
    "crawlee": "^3.11.0",
    "jsdom": "26.1.0",
    "puppeteer": "^23.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "turndown": "^7.2.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/turndown": "^5.0.0",
    "typescript": "^5.0.0",
    "bun-types": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "mcp",
    "browser",
    "automation",
    "scraping",
    "ai",
    "opencode"
  ],
  "author": "OpenCode Enhanced",
  "license": "MIT"
}
